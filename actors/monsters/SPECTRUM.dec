ACTOR SpectrumKnight : Enemy 13736
{
	//$Category Federation
   Health 225
   GibHealth 60
   Radius 20
   Height 56
   Mass 100
   Speed 30
   PainChance 50
   renderstyle add
   +MISSILEMORE
   +MISSILEEVENMORE
   +FASTER
   +BOSS
   +BRIGHT
   
   DropItem "Cell" 128
   DropItem "Cell" 64
   DropItem "Cell" 4
   
   Dropitem "Stimpack" 32
	Dropitem "ERASUREArmor" 4
	DropItem "ArmorShardSpawner" 64
   
   Dropitem "PlasmaRifle" 16
   
   DropItem "ShieldGenerator" 16
   
   Alpha 0.75
   Translation "0:255=4:4"
   BloodType "AeniBlood"
   SeeSound "Spectrum/Rainbows"
   AttackSound "monster/iniatk"
   PainSound "brain/pain"
   DeathSound "brain/death"
   ActiveSound "Wizard/Active"
   Obituary "The Spectrum Knight never wants to be with %o, or make believe with him." 
   Decal BulletChip
  states
  {
  Spawn:
    SKFA A 0 
    SKFA A 0 Thing_SetSpecial(0,226,642,0,random(30,40))
    SKFA E 1 A_LookEx(0,0,random(512,2048),random(1024,4096),random(90,120),"See")
    goto Spawn+2
  See:
    SKFA AABBCCDD 1 A_FastChase
    Goto See
  Missile:
    SKFA E 0 A_ChangeFlag("NoPain", 1)
    SKFA E 10 A_FaceTarget
    SKFA E 0 A_PlayWeaponSound("brain/spit")
    SKFA F 1 A_CustomMissile("SPECShotR", 31)
    SKFA F 1 A_CustomMissile("SPECShotO", 30)
    SKFA F 1 A_CustomMissile("SPECShotY", 29)
    SKFA F 1 A_CustomMissile("SPECShotG", 28)
    SKFA F 1 A_CustomMissile("SPECShotB", 27)
    SKFA F 1 A_CustomMissile("SPECShotI", 26)
    SKFA F 1 A_CustomMissile("SPECShotV", 25)
    SKFA E 5
    SKFA E 0 A_ChangeFlag("NoPain", 0)
    Goto See
  Pain:
    SKFA G 3
    SKFA G 3 A_BrainPain
    goto See
	Pain.KillMe:
		Goto See
  Death:
    SKFA H 0 A_JumpIfHealthLower(-60,"Splode")                   //
		SKFA H 5 A_AlertMonsters
		SKFA I 5 A_Scream
		Goto DeathFall
  Death.KnifeSlash:
  Death.KnifeStab:
  Death.KnifeSlashPower:
  Death.KnifeStabPower:
  Death.WakizashiSlash:
  Death.WakizashiStab:
  Death.WakizashiSlashPower:
  Death.WakizashiStabPower:
		SKFA H 5
		SKFA I 5 A_PlaySound("KNI_HIT")
	DeathFall:
    SKFA J 5 A_Fall 
    SKFA K 5 
    SKFA L 5
    SKFA M 5
    SKFA N 0 A_Jump(5, 2)
    SKFA N -1
    stop
    SKFA NNNNN 70
    SKFA NNNNN 70
    SKFA NNNNN 70
    SKFA N -1 Thing_Raise(0)
    Wait
  Splode:
    SKFA I 1                                                     // xDeath
    SKFA I 0 A_SpawnDebris("NashGore_Gib9",0)                    // GIBS!
    SKFA I 0 A_SpawnDebris("NashGore_Gib10",0)                   //
    SKFA I 0 A_SpawnDebris("NashGore_Gib11",0)                   //
    SKFA I 0 A_SpawnDebris("NashGore_Gib11",0)                   //
    SKFA I 0 A_SpawnItem("NashGore_GibGenerator",0,0,0,0)        //
    SKFA I 0                                                     //
    SKFA I 0 A_SpawnItem("NashGore_GibGenerator",0,0,0,0)        //
    SKFA I 0 A_XScream                                           //
    SKFA I 0 A_SpawnItem("NashGore_GibGenerator",0,0,0,0)        //
    SKFA I 0 A_NoBlocking                                        //
    SKFA I 0 A_SpawnItem("NashGore_GibGenerator",0,0,0,0)        //
    SKFA I 0                                                     //
    SKFA Z 0 A_SpawnItemEx("DEADStrikeKnightHead",0,0,0,0,0,0,0,SXF_TRANSFERTRANSLATION)//
    stop                                                         //
   Death.Fire2:
      TNT1 A 1 A_SpawnItem("BurnDeath1", 2, 1)
      stop
  Raise:
    SKFA KJ 5 
    SKFA IH 5  Thing_SetSpecial(0,226,642,0,random(30,40))        // RGA2 EXP COMPATIBILITY
    Goto See
  //Death.head:
  //  SKFA O 9 A_PlaySound("headshot/splat")                       //
  //  SKFA I 0 A_SpawnDebris("NashGore_Gib9",0)                    // GIBS!
  //  SKFA I 0 A_SpawnDebris("NashGore_Gib10",0)                   //
  //  SKFA I 0 A_SpawnDebris("NashGore_Gib11",0)                   //
  //  SKFA I 0 A_SpawnDebris("NashGore_Gib11",0)                   //
  //  SKFA P 9 A_XScream                                           //
  //  SKFA Q 9 A_Fall                                              //
  //  SKFA R 5                                                     //
  //  SKFA S 5                                                     //
  //  SKFA T 0 A_SpawnItemEx("DEADStrikeKnight",0,0,0,0,0,0,0,SXF_TRANSFERTRANSLATION)// FAKE BODY
  //  stop                                                         //
  }
}

