ACTOR HunterArmor : CustomInventory
{
  inventory.pickupmessage "Picked up the Hunter Armor!"
  translation "112:127=96:111"
  +VISIBILITYPULSE
  renderstyle translucent
  states
  {
  Spawn:
    FARM B 6
    FARM B 7 bright
    loop
  Pickup:
    TNT1 A 0 A_GiveInventory("HunterArmorItem", 1)
    TNT1 A 0 A_GiveInventory("HunterStealthItem", 1)
    Stop
  }
}

